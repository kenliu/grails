<mappings  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://xfire.codehaus.org/schemas/1.0/mapping.xsd">
    <mapping>              
    	<property name="id" ignore="true" />
    	<property name="title" />
    	<property name="url"  />    	    
    	<property name="rating" ignore="true"  />
    	<property name="dateCreated" nillable="true" ignore="true" />    	        	
    	<property name="notes" ignore="true" nillable="true" />
    	<property name="user" ignore="true" nillable="true"/>	
    	<property name="tags" ignore="true" componentType="org.grails.bookmarks.TagReference"/>   
   </mapping>      
</mappings>

